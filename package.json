{
  "name": "quick-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "browser-sync": "node ./browsersync.js",
    "build": "npm test && npm run webpack",
    "start": "PORT=$(get-port) concurrently \"npm run browser-sync\" \"npm run watch\"",
    "test": "standard",
    "watch": "concurrently \"npm run watch:test\" \"npm run webpack -- --watch\"",
    "watch:test": "onchange \"**/*.js\" -- npm test",
    "webpack": "webpack --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmednuaman/quick-react.git"
  },
  "keywords": [],
  "author": "Ahmed Nuaman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahmednuaman/quick-react/issues"
  },
  "homepage": "https://github.com/ahmednuaman/quick-react#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-css-modules": "^2.6.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "browser-sync": "^2.18.8",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "get-port-cli": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "onchange": "^3.2.1",
    "react-hot-loader": "next",
    "resolve-url-loader": "^2.0.2",
    "standard": "^10.0.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "standard": {
    "ignore": [
      "build"
    ]
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
